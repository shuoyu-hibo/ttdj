*{
    margin:0;
    padding:0;
}

body {
    font: 400 14px/1.5 "Microsoft YaHei","Hiragino Sans GB","WenQuanYi Micro Hei",tahoma,sans-serif;
    color: #ff6600;
}
html{
    background-color: #fff;
}
h1, h2, h3, h4, h5, h6 {
  font-family: "Hiragino Sans GB"," Microsoft YaHei","WenQuanYi Micro Hei",arial,sans-serif;
  color: #ff6600;
  -webkit-font-smoothing: antialiased;
}
.pg-floor {
    margin: 0 auto;
    min-width: 100%;
    background-color: #fff;
}
.site-mast__branding {
    position: relative;
    margin: 0 auto;
    height: 110px;
    width: 100%;
    background: #fff;
}
.site-mast__branding .cf{
    width:100%;
}
.site-mast{
    width: 100%;
    height: 150px;
}

.logoWrapper{
    width:1070px;
    margin:0 auto;
}

.site-mast__branding .site-logo {
    float: left;
    display: block;
    width: 100px;
    height: 100px;
    margin-top: 0;
    margin-left: 150px;
    background-image: url(../../image/xw/gg.jpg);
    background-repeat: no-repeat;
    background-position: 0;
}
.site-mast__branding p{
    margin-left: 340px;
    margin-top: 3px;
    font-size: 25px;
}
.site-mast__branding form{
    margin-left: 340px;
}
.site-mast__keep a {
    color:#ff6600;
    text-decoration: none;
}

.user-info__signup {
    padding: 0 10px 0 5px;
    color:#ff6600;
    border-right: 1px solid #ff6600;
}
.site-mast .dropdown__toggle {
    position: relative;
    display: inline-block;
    height: 16px;
    padding: 6px 20px 6px 12px;
    margin: 0 1px;
    color:#ff6600;
    z-index: 93;

}
.site-mast__user-w .user-orders a {
    color:#ff6600;
}
.search-box__button {
    float: left;
    width: 80px;
    margin: 0;
    height: 30px;
    line-height: 30px;
    border: none;
    color: #ff6600;
    background:skyblue;
    cursor: pointer;
    border-radius: 0;
}
.search-box__input {
    float: left;
    margin: 0;
    padding: 6px 5px;
    border: 2px solid #ff6600;
    border-right: none;
    border-left: none;
    border-radius: 0;
    width: 338px;
    height: 26px;
    line-height: 26px;
}
.site-mast__user-nav-w {
    height: 35px;
    font-size: 15px;
    border-top: 1px solid #ff6600;
    box-shadow: 0 0 0 1px #f60,0 0 0 2px #f60,0 0 5px 3px #f60;
}
.main{
    width:1070px;
    margin:0 auto;
}
.foot{
    width:100%;
    background: #fff;
    position: relative;
    margin:0 auto;
    color:#ff6600;
    border-top:2px solid #f60;
}
.img{
    width: 390px;
    height: 317px;
}
.sellinfo{
    width:1100px;
    height:420px;
}
.sellinfo1{
    width:270px;
    height:400px;
    float:left;
}
.tb-shop-info {
    position: relative;
    top: 3px;
    left: 1px;
}
.tb-shop-info-bg img{
    vertical-align: middle;
}
.tb-shop-info-wrap {
    padding: 0 10px 17px 20px;
    height: 378px;
    width: 235px;
    color: #ff6600;
    border:1px solid #ff6600;
}
.tb-shop-info a {
    color: #ff6600;
    text-decoration: none;
}
.tb-shop-info-hd .tb-shop-name {
    margin-top: 50px;
    height: 20px;
    overflow: hidden;
}
.tb-shop-info-hd .tb-shop-rank {
    margin-bottom: 5px;
    overflow: hidden;
}
.tb-shop-info-hd .tb-shop-seller {
    overflow: hidden;
    margin-bottom: 5px;
    line-height: 20px;
}
.tb-shop-info-hd .tb-shop-ww {
    height: 22px;
    margin-bottom: 5px;
}
.tb-shop-info-bd {
    margin-top: 80px;
    padding-top: 13px;
    border-top: dotted 1px #ff6600;
    overflow: hidden;
}
.tb-shop-info-bd .tb-shop-rate {
    overflow: hidden;
    margin: 0;
}
.tb-shop-info-bd .tb-shop-rate dl {
    display: inline;
    float: left;
    padding-left: 5px;
    margin-left: 5px;
    border-left: 1px solid #ff6600;
}
.tb-shop-info-ft a {
    display: inline;
    float: left;
    margin-right: 5px;
    margin-top: 20px;
    width: 74px;
    height: 28px;
    text-align: center;
    color: #ff6600;
    background:skyblue;
    line-height: 28px;
    border: 1px solid #ff6600;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    -ms-border-radius: 2px;
    -o-border-radius: 2px;
    border-radius: 2px;
}
.common-rating {
    vertical-align: bottom;
    display: block;
    float: right;
}
.sellinfo2{
    width:800px;
    height:400px;
    float: left;
}

.recmUp{
    font-size: 25px;
    color:#ff6600;
}

.downLeft{
	float:left;
	width:380px;
	height:360px;
}

.downRight{
	float:right;
	width:380px;
	height:360px;
}
.span4{
	width:360px;
}
.recmInfo{
	margin-left:30px;
}

.uploadPic{
    float:right;
    clear:both;
}
.btn {
	  color: #f60;
	  background-color: #fff;
	  border-color: #f60;
	  background-image: linear-gradient(to bottom,skyblue,skyblue);
}

.category-floor__body cf{
	width:1070px;
}



.category-floor__head{
	height:80px;
	width:1060px;
	border:3px solid #f60; 
}

.common-rating {
    vertical-align: bottom;
    display: block;
    margin:10px 130px 0 0;
    float:right;
}

.mt-slider {
    position: relative;
}

.index-slider .head h2 {
    line-height: 20px;
    font-size: 14px;
    color: #ff6600;
}
.category-floor {
    margin-bottom: 20px;
}


.deal-tile {
    position: relative;
    float: left;
    width: 350px;
    border: 1px solid #974e1e;
    background-color: #101010;
    color: #ff6600;
}

.deal-tile__extra .extra-inner {
    position: relative;
    height: 33px;
    padding: 0 15px;
    font-size: 12px;
    background-color: #2c2c2c;
}

.J-webp{
    background: url("../../image/xw/default.png");
}

.price {
    font-family: arial,sans-serif;
    color: #f76120;
    margin-left: 35px;
}
.site-info-w {
    border-top: 3px solid #ff6600;
    padding-bottom: 30px;
    clear: both;
}
.site-info {
    margin: 0 auto;
    width: 980px;
    font-size: 12px;
    clear: both;
}
.site-info__item {
    float: left;
    border-left: 1px dotted #161516;
    border-right: 1px dotted #161818;
    padding: 20px 0 0 25px;
    width: 161px;
    height: 160px;
}
.site-info__item h3 {
    margin-bottom: 8px;
    font-size: 14px;
    color: #ff6600;
}
.site-info__item a {
    color: #ff6600;
}
.site-info__item--service {
    border-right-color: #161516;
    padding-right: 25px;
    width: 176px;
    font-family: "Hiragino Sans GB"," Microsoft YaHei","WenQuanYi Micro Hei",arial,sans-serif;
    color: #ff6600;
}
.copyright {
    padding: 30px 0 10px;
    clear: both;
    color: #ff6600;
    text-align: center;
    font-size: 12px;
    font-family: initial;
}
.cert {
    margin: 10px auto;
    text-align: center;
}