			        		return;
			        	}
		       		});
	       		}
	    	});
	    }
	    
	    loadProduce();//load produceList
	});
	
	
    function checkInputStore(inputId,inputName){//checkinput for store
    	if($("#"+inputId).val()=='' || $("#"+inputId).val()==undefined){
