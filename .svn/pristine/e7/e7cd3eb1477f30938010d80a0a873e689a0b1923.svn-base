<script type="text/javascript">

 $(function(){
 	$("#myModal1").modal("show");
 	$("#saveShop").click(function(){
	    	if(!checkInputStore("storeName","商铺名称")){
	    		return false;
	    	}
	    	if(!checkInputStore("address","地址")){
	    		return false;
	    	}
	    	if(!checkInputStore("linkNumber","联系电话")){
	    		return false;
	    	}
	    	if(!checkInputStore("province","省份")){
	    		return false;
	    	}
	    	if(!checkInputStore("city","城市")){
	    		return false;
	    	}
	    	var sul = "us!saveStoreInfo.do?provinceId="+$("#province option:selected").attr("id");
	    	sul += "&cityId="+$("#city option:selected").attr("id");
	    	if($("#county option:selected").attr("id")!=undefined){
	    		sul += "&countyId="+$("#county option:selected").attr("id");
	    	}
	    	if($("#county option:selected").attr("id")!=undefined){
		    	sul += "&businessGroupId="+$("#businessGroup option:selected").attr("id");
	    	}
	    	sul += "&";
    		var strUrl = generateUrl(sul,"storeName,address,linkNumber,province,city,county,businessGroup");
    		//alert(strUrl);
    		$.post(strUrl,function(json){
				if(json["result"]=="0"){
					//$("#myModal1").modal("hide");
					$("#viewStoreName").html($("#storeName").val());
					var vStoreLeve = 1;
					$("#viewStoreleve").css("width",""+vStoreLeve*20+"%");
					$("#viewAddress").html($("#address").val());
					$("#viewLinkNumber").html($("#linkNumber").val());
				}else{
					$("#warningStore").html('<strong>提示:</strong> 商品添加失败!');
    				$("#warningStore").toggle(true);
    				return false;
				}
			});
	    });
	    
	    
        //$("#myModal1").css("z-index","1051");
        //load province city county and bgrounp start
        $.post(generateUrl("ct!getProvince.do?"),function(data){
        	$("#province").html("<option></option>");
        	$.each(data["record"],function(i,n){
        		$("<option id="+n.provinceCode+"\>"+n.provinceName+"</option>").appendTo($("#province"));
        	});
        });
        $("#province").change(function(){
        	var sU = generateUrl("ct!getCity.do?provinceId="+$("#province option:selected").attr("id"))
        	$("#city").removeAttr("disabled");
        	$("#city").html("<option></option>");
        	$.post(sU,function(data){
	        	$.each(data["record"],function(i,n){
	        		$("<option id="+n.cityCode+"\>"+n.cityName+"</option>").appendTo($("#city"));
        		});
       		});
       		$("#city").change();
        });
        $("#city").change(function(){
        	var sU = generateUrl("ct!getCounty.do?cityId="+$("#city option:selected").attr("id"))
        	$("#county").removeAttr("disabled");
        	$("#county").html("<option></option>");
        	$.post(sU,function(data){
	        	$.each(data["record"],function(i,n){
	        		$("<option id="+n.countyId+"\>"+n.countyName+"</option>").appendTo($("#county"));
        		});
       		});
       		$("#county").change();
        });
        $("#county").change(function(){
        	var sUl = generateUrl("ct!getBusinessGroup.do?cityId="+$("#city option:selected").attr("id")+"&countyId="+$("#county option:selected").attr("id"))
        	$("#businessGroup").removeAttr("disabled");
        	$("#businessGroup").html("<option></option>");
        	$.post(sUl,function(data){
	        	$.each(data["record"],function(i,n){
	        		$("<option id="+n.busiGropId+"\>"+n.busiGropName+"</option>").appendTo($("#businessGroup"));
        		});
       		});
        });
		//load province city county and bgrounp end
		
 });
</script>
