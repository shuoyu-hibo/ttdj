    $(function(){
		$("#pageloading").hide();
		$("#imgUpload").attr("src","../../image/addPic.png");
		$("#warning").hide();
		$("#myModal").css("z-index","-1");
		$.post(generateUrl("us!getUserName.do?"),function(data){
			$("#userNameShow").html(data["record"][0]);
		});
		//$("#quit").attr("href",generateUrl("login!userQuit.do?"));
