			if(confirm("确认退出吗？")){
				window.location.href=generateUrl("login!userQuit.do?");
			}
		});
		$.post(generateUrl("us!ifBusinessman.do?"),function(data){
			if(data["result"]!=0){
				$("#myModal1").modal({
			        show:true,
			        backdrop:"static"
		        });
			}else{
				//alert(data["record"][0].userName);
				$("#myModal1").css("z-index",-1);
				$("#viewStoreName").html(data["record"][0].storeName);
				var vStoreLeve = "";
				if(data["record"][0].storeLeve==null||data["record"][0].storeLeve==""){
					vStoreLeve = 1;
				}else{
					vStoreLeve = parseInt(data["record"][0].storeLeve);
				}
				$("#viewStoreleve").css("width",""+vStoreLeve*20+"%");
				$("#viewAddress").html(data["record"][0].address);
				$("#viewLinkNumber").html(data["record"][0].linkNumber);
			}
		});
        //$("#myModal1").css("z-index","1051");
        //load province city county and bgrounp start
        $.post(generateUrl("ct!getProvince.do?"),function(data){
        	$("#province").html("<option></option>");
        	$.each(data["record"],function(i,n){
        		$("<option id="+n.provinceCode+"\>"+n.provinceName+"</option>").appendTo($("#province"));
        	});
        });
        $("#province").change(function(){
        	var sU = generateUrl("ct!getCity.do?provinceId="+$("#province option:selected").attr("id"))
        	$("#city").removeAttr("disabled");
        	$("#city").html("<option></option>");
        	$.post(sU,function(data){
	        	$.each(data["record"],function(i,n){
	        		$("<option id="+n.cityCode+"\>"+n.cityName+"</option>").appendTo($("#city"));
        		});
       		});
       		$("#city").change();
        });
        $("#city").change(function(){
        	var sU = generateUrl("ct!getCounty.do?cityId="+$("#city option:selected").attr("id"))
        	$("#county").removeAttr("disabled");
        	$("#county").html("<option></option>");
        	$.post(sU,function(data){
	        	$.each(data["record"],function(i,n){
	        		$("<option id="+n.countyId+"\>"+n.countyName+"</option>").appendTo($("#county"));
        		});
       		});
       		$("#county").change();
        });
        $("#county").change(function(){
        	var sUl = generateUrl("ct!getBusinessGroup.do?cityId="+$("#city option:selected").attr("id")+"&countyId="+$("#county option:selected").attr("id"))
        	$("#businessGroup").removeAttr("disabled");
        	$("#businessGroup").html("<option></option>");
        	$.post(sUl,function(data){
	        	$.each(data["record"],function(i,n){
	        		$("<option id="+n.busiGropId+"\>"+n.busiGropName+"</option>").appendTo($("#businessGroup"));
        		});
       		});
        });
		//load province city county and bgrounp end
		
		$("#save").click(function(){
	    	if(!checkInput("produceName","商品名称")){
	    		return;
	    	}
	    	if(!checkInput("produceDesc","商品描述")){
	    		return;
	    	}
	    	if(!checkInput("producePrice","商品价格")){
	    		return;
	    	}
	    	if($("#picUrl").val()==''){
	    		$("#warning").html('<strong>提示:</strong> 请选择要上传的图片!');
	    		$("#warning").toggle(true);
	    	}else{
	    		$("#warning").toggle(false);
	    		var strUrl = generateUrl("produce!saveProduceData.do?","produceName,produceDesc,producePrice,picUrl");
	    		$.post(strUrl,function(json){
					if(json["result"]=="0"){
						//alert(1);
						//$("#ok").show();
						$("#myModal").modal("hide");
						loadProduce();
					}else{
						$("#warning").html('<strong>提示:</strong> 商品添加失败!');
	    				$("#warning").toggle(true);
					}
				});
	    	}
	    });
	    
	    $("#saveShop").click(function(){
	    
	    	if(!checkInputStore("storeName","商铺名称")){
	    		return;
	    	}
	    	if(!checkInputStore("address","地址")){
	    		return;
	    	}
	    	if(!checkInputStore("linkNumber","联系电话")){
	    		return;
	    	}
	    	if(!checkInputStore("province","省份")){
	    		return;
	    	}
	    	if(!checkInputStore("city","城市")){
	    		return;
	    	}
	    	var sul = "us!saveStoreInfo.do?provinceId="+$("#province option:selected").attr("id");
	    	sul += "&cityId="+$("#city option:selected").attr("id");
	    	if($("#county option:selected")==undefined){
	    		sul += "&countyId="+$("#county option:selected").attr("id");
	    	}
	    	if($("#county option:selected")==undefined){
		    	sul += "&businessGroupId="+$("#businessGroup option:selected").attr("id");
	    	}
	    	sul += "&";
    		var strUrl = generateUrl(sul,"storeName,address,linkNumber,province,city,county,businessGroup");
    		//alert(strUrl);
    		$.post(strUrl,function(json){
				if(json["result"]=="0"){
					$("#myModal1").modal("hide");
					$("#viewStoreName").html($("#storeName").val());
					var vStoreLeve = 1;
					$("#viewStoreleve").css("width",""+vStoreLeve*20+"%");
					$("#viewAddress").html($("#address").val());
					$("#viewLinkNumber").html($("#linkNumber").val());
				}else{
					$("#warningStore").html('<strong>提示:</strong> 商品添加失败!');
    				$("#warningStore").toggle(true);
				}
			});
	    });
	    for(var i=0;i<9;i++){
 	    	$("#itemUpdate"+(i+1)).click(function(){
 	    		updId=$(this).attr("name");
 	    		$("#modalUpdDiv").load(genLinkUrl("html/home/modalPdUpdate.html?produceId="+updId));
 	    	});
	    	$("#itemDele"+(i+1)).click(function(){
	    		if(confirm("确定要删除该商品吗?")){
	   				var sUl = generateUrl("produce!deleteProduceData.do?produceId="+$(this).attr("name"));
	    			$.post(sUl,function(data){
			        	if(data["result"]=="0"){
			        		if(loadProduce()){
			        			return;
			        		}
			        	}else{
			        		alert("商品删除失败！");
			        		return;
			        	}
		       		});
	       		}
	    	});
	    }
	    
	    loadProduce();//load produceList
	});
	
	
    /** 
     * upload Picture
     */  
    function uploadUnitImage()  {  
    		$("#pageloading").show();
            if($("#fileToUpload").val()==''){
            	$("#warning").show();
            	$("#pageloading").hide();
            	return;
            }
             $.ajaxFileUpload({  
                 url:generateUrl("produce!uploadImage.do?"),             //需要链接到服务器地址  
                 secureuri:false,  
                 fileElementId:'fileToUpload',                         //文件选择框的id属性  
                 dataType: 'json',                                     //服务器返回的格式，可以是json  
                 success: function (data, status)             //相当于java中try语句块的用法  
                 {     
                 //alert(data);       //data是从服务器返回来的值     
                	 $("#pageloading").hide();
                     $('#result').html(data["msg"]);  
                     var imgPath="../../uploadfiles/"+data["newname"];
   					 $("#imgUpload").attr("src",imgPath);
   					 $("#picUrl").val(imgPath);
   					 $("#warning").hide();
   					 return 0;
                 },  
                 error: function (data, status, e)             //相当于java中catch语句块的用法  
                 {  
                 	$("#pageloading").hide();
                     $('#result').html(data["msg"]);  
                     $("#warning").hide();
                     return 1;
                 }  
               }  
             );  
    }  
    
    function checkInput(inputId,inputName){//checkinput for produceinfo
    	if($("#"+inputId).val()=='' || $("#"+inputId).val()==undefined){
	    		$("#warning").html("<strong>提示:</strong> 请输入"+inputName+"!");
	    		$("#warning").show();
	    		$("#ok").hide();
	    		return false;
	    }
	    $("#warning").hide();
	    return true;
    }
    
    function checkInputStore(inputId,inputName){//checkinput for store
    	if($("#"+inputId).val()=='' || $("#"+inputId).val()==undefined){
	    		$("#warningStore").html("<strong>提示:</strong> 请输入"+inputName+"!");
	    		$("#warningStore").show();
	    		$("#okStore").hide();
	    		return false;
	    }
	    $("#warningStore").hide();
	    return true;
    }
    
    function loadProduce(){
    	$.post(generateUrl("produce!getProduceInfoByUser.do?"),function(data){//load produce
	   		var pdCount = parseInt(data["recordSize"]);
	   		if(pdCount<9){
	   			for(var i=pdCount;i<9;i++){
	   				$("#item"+(i+1)).hide();
	   				$("#itemImg"+(i+1)).attr("src","");
		    		$("#itemPdName"+(i+1)).html("");
		    		$("#itemPdDesc"+(i+1)).html("");
		    		$("#itemPdPrice"+(i+1)).html("");
		    		$("#itemUpdate"+(i+1)).attr("onclick","");
		    		$("#itemDele"+(i+1)).click(function(){});
	   			}
	   		}
	    	$.each(data["record"],function(i,n){
	    		$("#item"+(i+1)).show();
	    		$("#itemImg"+(i+1)).attr("src",n.producePic);
	    		$("#itemPdName"+(i+1)).html(n.produceName);
	    		$("#itemPdDesc"+(i+1)).html(n.produceDesc);
	    		$("#itemPdPrice"+(i+1)).html(n.producePrice);
	    		$("#itemUpdate"+(i+1)).attr("name",n.produceId);
	    		$("#itemDele"+(i+1)).attr("name",n.produceId);
	    	});
    		pageNumSet(data["recordSize"]);
    	});
    	return true;
    }
    var pageCurrent=1;//当前页面
    function pageNumSet(recordSize){//page btn create
    	var str = ""+recordSize/9;
		var pageSize = 0;
		if(recordSize%9==0){
			pageSize = recordSize/9;
		}else{
			var num = str.split(".")[0];
			pageSize = parseInt(num)+1;
		}
		//if(recordSize%9>=1&&recordSize>9){
	    //		pageSize+=1;
    	//}
    	for(var i=1;i<=5;i++){$("#pNum"+i).show();}
    	$("#pNumPre").show();
    	$("#pNumNext").show();
    	if(pageSize==1){//如果是一页
    		for(var i=1;i<=5;i++){$("#pNum"+i).hide();}
	     	$("#pNumPre").hide();
	     	$("#pNumNext").hide();
    	}
    	for(pageSize;pageSize<5;pageSize++){
   			$("#pNum"+(pageSize+1)).hide();
    	}
	}
    
</script>
