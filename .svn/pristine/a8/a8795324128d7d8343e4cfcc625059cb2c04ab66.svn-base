<script type="text/javascript">
$(function(){
	var produceId = getUrlParam("produceId");
	if(produceId=='undefined' || produceId==''){
		return;
	}
	$.post(generateUrl("sh!getProduceInfoById.do?produceId="+produceId),function(data){
		//$(".productPic").css({"background-image":"url(\""+data["record"][0].producePic+"\")", "background-size":"100% 100%"});
		$(".productPic").attr("src",data["record"][0].producePic);
		$("#producePrice").text(data["record"][0].producePrice);
		$("#remitPrice").text(data["record"][0].remitPrice);
		$("#finalPrice").text(data["record"][0].finalPrice);
		$("#viewProduceName").text(data["record"][0].produceName);
		$("#viewProduceDesc").text(data["record"][0].produceDesc);
		$("#viewStoreName").text(data["record"][0].storeName);
		$("#viewStoreLeve").text(data["record"][0].storeLeve);
		$("#viewAddress").text(data["record"][0].address);
		$("#viewLinkNumber").text(data["record"][0].linkNumber);
	});
	
    $("#ratingVal").webwidget_rating_simple({
        rating_star_length: '5',
        rating_initial_value: '',
        rating_function_name: 'rateChangeVal',//this is function name for click
        directory: '../../image'
    });
    
});

	function rateChangeVal(){
		$("#ratingValShow").text($("#ratingVal").val());
	}

</script>
